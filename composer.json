{
	"name": "imaga/imagagc",
	"type": "wordpress-theme",
	"license": "GPL-2.0-or-later",
	"description": "Child theme for Genesis Framework with a modern development workflow",
	"homepage": "https://imagagc.imaga.nl",
	"authors": [
		{
			"name": "IMAGA",
			"email": "Wordpress@imaga.nl",
			"homepage": "https://imaga.nl/"
		}
	],
	"keywords": [
		"wordpress",
		"genesis"
	],
	"require": {
		"php": ">=7.3",
		"composer/installers": "*",
		"wpackio/enqueue": "^2.2.0"
	},
	"require-dev": {
        "squizlabs/php_codesniffer": "*",
        "wp-coding-standards/wpcs": "*",
        "phpcompatibility/php-compatibility": "*",
		"dealerdirect/phpcodesniffer-composer-installer": "*",
		"roave/security-advisories": "*",
		"dealerdirect/qa-tools ": "*"
	},

	"scripts": {
		"phpcs": " ./vendor/bin/phpcs ",
		"phpcbf": "./vendor/bin/phpcbf --report=diff -vv"
	}
}
